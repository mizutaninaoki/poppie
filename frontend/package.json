{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export -o ../public",
    "start": "PORT=3000 next start",
    "lint": "next lint",
    "make-e2e-env": "cp .env.e2e .env.production",
    "cy:run": "TZ=Asia/Tokyo cypress run",
    "cy:run:dev": "TZ=Asia/Tokyo cypress run --env CYPRESS_ENV=dev",
    "cy:open": "cypress open --env CYPRESS_ENV=dev",
    "gen": "cd .. && ./manage.py graphql_schema --schema config.schema.schema --out app/graphql/schema.graphql && cd - && graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "classnames": "^2.3.1",
    "daisyui": "^2.14.1",
    "date-fns": "^2.28.0",
    "graphql": "^16.3.0",
    "js-cookie": "^3.0.1",
    "libphonenumber-js": "^1.10.19",
    "lodash.groupby": "^4.6.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-cookie": "^4.1.1",
    "react-dom": "17.0.2",
    "react-icons": "^4.7.1",
    "recharts": "^2.1.10",
    "sass": "^1.49.9",
    "uuid": "^8.3.2",
    "zod": "^3.13.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/typescript": "^2.4.7",
    "@graphql-codegen/typescript-operations": "^2.3.4",
    "@graphql-codegen/typescript-react-apollo": "^3.2.10",
    "@testing-library/cypress": "^9.0.0",
    "@types/classnames": "^2.3.1",
    "@types/js-cookie": "^3.0.1",
    "@types/lodash.groupby": "^4.6.6",
    "@types/node": "^18.13.0",
    "@types/react": "17.0.39",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.2",
    "cypress": "^12.5.1",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.8",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
}
